{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "storybook": "start-storybook -p 6006",
    "storycap": "storycap http://localhost:6006",
    "build-storybook": "build-storybook",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "gen:component": "npx hygen component new",
    "gen:state": "npx hygen state new",
    "gen:post": "npx hygen post new",
    "gen:ogp-component": "ts-node tools/ogp/generateComponent.ts",
    "gen:ogp-image": "ts-node tools/ogp/generateOgpImages.ts",
    "gen:ogp": "npm run gen:ogp-component && npm run storycap && npm run gen:ogp-image"
  },
  "dependencies": {
    "@weknow/gatsby-remark-codepen": "^0.1.0",
    "babel-plugin-styled-components": "^1.10.6",
    "gatsby": "^2.18.11",
    "gatsby-image": "^2.2.36",
    "gatsby-plugin-catch-links": "^2.1.21",
    "gatsby-plugin-google-analytics": "^2.1.31",
    "gatsby-plugin-layout": "^1.1.18",
    "gatsby-plugin-manifest": "^2.2.33",
    "gatsby-plugin-offline": "^3.0.29",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^3.1.18",
    "gatsby-plugin-remove-console": "^0.0.2",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^2.3.7",
    "gatsby-plugin-sitemap": "^2.2.24",
    "gatsby-plugin-styled-components": "^3.1.16",
    "gatsby-plugin-twitter": "^2.1.17",
    "gatsby-plugin-typescript": "^2.1.23",
    "gatsby-remark-autolink-headers": "^2.1.21",
    "gatsby-remark-code-titles": "^1.1.0",
    "gatsby-remark-copy-linked-files": "^2.1.33",
    "gatsby-remark-embed-youtube": "^0.0.7",
    "gatsby-remark-images": "^3.1.38",
    "gatsby-remark-prismjs": "^3.3.27",
    "gatsby-remark-responsive-iframe": "^2.2.30",
    "gatsby-source-filesystem": "^2.1.42",
    "gatsby-transformer-remark": "^2.6.42",
    "gatsby-transformer-sharp": "^2.3.9",
    "immer": "^5.0.1",
    "mikanjs": "^1.0.11",
    "mustache": "^3.1.0",
    "prismjs": "^1.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "styled-components": "^4.4.1",
    "styled-normalize": "^8.0.6",
    "typescript": "^3.7.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@storybook/addon-a11y": "^5.2.8",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-docs": "^5.2.8",
    "@storybook/addon-info": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addon-storysource": "^5.2.8",
    "@storybook/addon-viewport": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/preset-typescript": "^1.2.0",
    "@storybook/react": "^5.2.8",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.0.24",
    "@types/node": "^13.1.2",
    "@types/react": "^16.9.16",
    "@types/react-helmet": "^5.0.14",
    "@types/react-redux": "^7.1.5",
    "@types/signale": "^1.2.1",
    "@types/storybook__addon-info": "^5.2.1",
    "@types/styled-components": "^4.4.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "fs-extra": "^8.1.0",
    "hygen": "^5.0.3",
    "jest": "^24.9.0",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "jest-resolve": "^24.9.0",
    "jest-watch-typeahead": "^0.4.2",
    "prettier": "^1.19.1",
    "react-docgen-typescript-loader": "^3.6.0",
    "rimraf": "^3.0.0",
    "storycap": "^2.3.1",
    "textlint": "^11.5.0",
    "ts-node": "^8.5.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$",
      "node_modules/(?!react-native|lodash-es)"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}
